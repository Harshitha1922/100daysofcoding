int maxDepth(string s) {
	// Write your code here.
	int c1 = 0;
	int maxi=INT_MIN;
	for(int i =0;i<s.size();i++){
		maxi=max(maxi,c1);
		if(s[i] == '('){
			c1++;
		}
		if(s[i] == ')'){
			c1--;
		}

	}
	return maxi;
}
